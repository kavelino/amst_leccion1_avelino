<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity" >

    <Button
        android:id="@+id/button_sidweb"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="240dp"
        android:scaleX="@android:dimen/notification_large_icon_width"
        android:text="Sidweb"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="105dp" />

    <Button
        android:id="@+id/button_academico"
        android:layout_width="202dp"
        android:layout_height="50dp"
        android:text="Academico"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="305dp" />

    <Button
        android:id="@+id/button_bienestar"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="@string/bienestar"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="442dp" />

    <Button
        android:id="@+id/button_vinculos"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Vinculos"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="376dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="212dp"
        android:layout_height="68dp"
        app:srcCompat="@drawable/logo_espol"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="94dp" />

</androidx.constraintlayout.widget.ConstraintLayout>

